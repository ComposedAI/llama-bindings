{
  "name": "llama-bindings",
  "version": "0.0.1",
  "description": "Node bindings for llama.cpp",
  "main": "./dist/index.js",
  "scripts": {
    "build": "cmake-js rebuild && tsc",
    "clean": "cmake-js clean && tsc --build --clean",
    "start": "node ./dist/index.js",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "prettier:check": "prettier --check \"src/**/*.ts\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "llama",
    "llama.cpp",
    "ai",
    "llm"
  ],
  "author": "Nick Nance",
  "license": "MIT",
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "cmake-js": "^7.2.1",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
